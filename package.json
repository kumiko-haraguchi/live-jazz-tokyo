{
  "name": "live-jazz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node server",
    "test": "yarn lint && yarn test:backend && CI=true yarn test:frontend",
    "test:backend": "NODE_ENV=test mocha --recursive server/test --opts server/test/mocha.opts",
    "test:frontend": "react-scripts test --env=jsdom",
    "db:nuke": "dropdb 'livejazz' && createdb 'livejazz' && yarn migrate && yarn seed",
    "backend": "nodemon server",
    "frontend": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "migrate": "knex migrate:latest --knexfile knexfile.js",
    "rollback": "knex migrate:rollback --knexfile knexfile.js",
    "seed": "knex seed:run --knexfile knexfile.js",
    "postinstall": "yarn build",
    "heroku-postbuild": "yarn rollback && yarn migrate && yarn seed"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "can-use-dom": "^0.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "force-ssl-heroku": "^1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.0.1",
    "knex": "^0.13.0",
    "material-ui": "^0.19.2",
    "morgan": "~1.8.1",
    "pg": "^7.3.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-google-maps": "^8.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "react-stripe-elements": "^1.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "stripe": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "mocha": "3.5.0",
    "nodemon": "^1.12.0",
    "querystring": "^0.2.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^3.3.0"
  },
  "engines": {
    "node": ">=8.2.1"
  },
  "proxy": "http://localhost:3001"
}
